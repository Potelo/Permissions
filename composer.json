{
    "name": "foxted/permissions",
    "description": "Tiny Laravel 4 package for handling user roles and permissions based on the work of mrterryh/permissions",
    "keywords": ["laravel", "permissions", "auth", "roles"],
    "license": "MIT",
    "authors": [
        {
            "name": "Terry Harvey",
            "email": "mcstormify@gmail.com"
        },
        {
            "name": "Valentin Prugnaud",
            "email": "valentin@whatdafox.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "1.*",
        "illuminate/foundation":"1.*"
    },
    "require-dev":{
        "phpspec/phpspec": "2.1.*@dev"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Foxted\\Permissions\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
